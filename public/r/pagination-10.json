  "files": [
    {
      "path": "src/components/customized/pagination/pagination-10.tsx",
      "content": "import { ChevronsLeft, ChevronsRight } from \"lucide-react\";\nimport {\n  Pagination,\n  PaginationContent,\n  PaginationItem,\n  PaginationLink,\n} from \"@/registry/ui/pagination\";\n\nexport default function PaginationWithIconAndLabel() {\n  return (\n    <Pagination>\n      <PaginationContent>\n        <PaginationItem>\n          <PaginationLink\n            aria-label=\"Go to previous page\"\n            className=\"gap-1 pl-2.5\"\n            href=\"#\"\n            size=\"default\"\n          >\n            <ChevronsLeft className=\"h-4 w-4\" />\n            <span>Previous</span>\n          </PaginationLink>\n        </PaginationItem>\n        <PaginationItem>\n          <PaginationLink href=\"#\">1</PaginationLink>\n        </PaginationItem>\n        <PaginationItem>\n          <PaginationLink href=\"#\" isActive>\n            2\n          </PaginationLink>\n        </PaginationItem>\n        <PaginationItem>\n          <PaginationLink href=\"#\">3</PaginationLink>\n        </PaginationItem>\n        <PaginationItem>\n          <PaginationLink\n            aria-label=\"Go to next page\"\n            className=\"gap-1 pr-2.5\"\n            href=\"#\"\n            size=\"default\"\n          >\n            <span>Next</span>\n            <ChevronsRight className=\"h-4 w-4\" />\n          </PaginationLink>\n        </PaginationItem>\n      </PaginationContent>\n    </Pagination>\n  );\n}\n",
      "type": "registry:component"
    }
  ]
}